import React from 'react';
import { create } from "react-test-renderer"
import LocalePerson from '../LocalePerson';

describe ("ButtonPane Component", () => {
    test("Clicking New will create a new row", () => {
        const component = create(<LocalePerson />);
        const rootInstance = component.root;
        const Button = rootInstance.findByType("button");
        console.log(Button)
    })   
})
